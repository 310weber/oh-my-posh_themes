{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#8a8a8a",
          "template": "\u256d",
          "style": "plain",
          "type": "text"
        },
        {
          "background": "#002030",
          "foreground": "#f00060",
          "leading_diamond": "\ue0b6",
          "properties": {
            "windows": "\uf179"
          },
          "style": "diamond",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "type": "os"
        },
        {
          "background": "#002030",
          "foreground": "#0075af",
          "trailing_diamond": "\ue0b0",
          "style": "diamond",
          "template": " \u23a8{{.Shell}}\u23ac ",
          "type": "text"
        },
        {
          "background": "#0087D8",
          "foreground": "#003544",
          "leading_diamond": "<transparent,background>\ue0b0",
          "trailing_diamond": "\ue0b0",
          "properties": {
            "folder_separator_icon": "/",
            "style": "full"
          },
          "style": "diamond",
          "template": " \ue5ff {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#33DD2D",
          "background_templates": [
            "{{ if gt .Code 0 }}#f1184c{{ end }}"
          ],
          "foreground": "#242424",
          "leading_diamond": "<transparent,background>\ue0b0",
          "trailing_diamond": "\ue0b0",
          "properties": {
            "always_enabled": false
          },
          "style": "diamond",
          "template": " \uea6c ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#d2ff5e",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ff9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#f17c37{{ end }}"
          ],
          "foreground": "#193549",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,background>\ue0b2",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "branch_icon": "󰘬 "
          },
          "style": "diamond",
          "template": " {{ .HEAD }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "#0088ae",
          "background": "#005167",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,background>\ue0b2",
          "style": "diamond",
          "template": "  {{ round .PhysicalPercentUsed .Precision }}% ",
          "properties": {
            "precision": 2
          },
          "type": "sysinfo"
        },
        {
          "background": "#003543",
          "foreground": "#fff",
          "invert_powerline": true,
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4",
          "style": "diamond",
          "template": " {{ .CurrentDate | date .Format}}",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#8a8a8a",
          "template": "\u2570",
          "style": "plain",
          "type": "text"
        },
        {
          "foreground": "#FFD700",
          "style": "plain",
          "template": " \u26a1 ",
          "type": "root"
        },
        {
          "foreground": "#8a8a8a",
          "style": "plain",
          "template": " <b>\u0024</b>",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#FFD700",
    "template": "{{if .Root}}\u26a1 {{end}}<b>> </b>"
  },
  "version": 2
}
